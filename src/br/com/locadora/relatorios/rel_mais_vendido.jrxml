	<queryString>
		<![CDATA[SELECT
    *
FROM
    (SELECT
        CODIGO_PRODUTO,
            NOME_PRODUTO AS DESCRICAO,
            SUM(QUANTIDADE) AS QUANTIDADE
    FROM
        ITEM_VENDA A, PRODUTO B
    WHERE
        A.PRODUTO_CODIGO_PRODUTO = B.CODIGO_PRODUTO
            AND DATA_LANCAMENTO BETWEEN '2015-01-01' AND '2015-03-01'
            AND TYPE_PRODUCT = 1
            AND NOME_PRODUTO LIKE '%%'
    GROUP BY PRODUTO_CODIGO_PRODUTO UNION ALL SELECT
        CODIGO_PACOTE_PROMOCIONAL AS CODIGO_PRODUTO,
            DESCRICAO,
            SUM(QUANTIDADE) AS QUANTIDADE
    FROM
        ITEM_VENDA IV, PACOTE_PROMOCIONAL PP
    WHERE
        IV.PACOTE_PROMOCIONAL_CODIGO_PACOTE_PROMOCIONAL = PP.CODIGO_PACOTE_PROMOCIONAL
            AND DATA_LANCAMENTO BETWEEN '2015-01-01' AND '2015-03-01'
            AND TYPE_PRODUCT = 0
            AND DESCRICAO LIKE '%%'
    GROUP BY CODIGO_PACOTE_PROMOCIONAL) AS CONSULTA
ORDER BY QUANTIDADE DESC]]>
	</queryString>
