	<queryString>
		<![CDATA[SELECT
    DATA_VENCIMENTO,
    CODIGO_LANCAMENTO_CONTA,
    CODIGO_FORNECEDOR,
    NOME_FANTASIA AS ORIGEM_DESTINO,
    DESCRICAO,
    DOCUMENTO,
    VALOR_A_PAGAR,
    VALOR_PAGO,
    (CASE WHEN (VALOR_A_PAGAR - VALOR_PAGO) < 0 THEN 0 ELSE
    VALOR_A_PAGAR - VALOR_PAGO END ) AS DEBITO,
    DATA_PAGAMENTO,
    C.NOME_USUARIO,
    A.CAIXA_CODIGO_CAIXA, CATEGORIA_CODIGO_CATEGORIA
FROM
    locadora.lancamento_conta A,
    FORNECEDOR B,
    USUARIO C
WHERE
    A.FORNECEDOR_CODIGO_FORNECEDOR = B.CODIGO_FORNECEDOR
        AND A.USUARIO_CODIGO_USUARIO = C.CODIGO_USUARIO
        AND DATA_VENCIMENTO BETWEEN '2015-01-01' AND '2015-03-01'
        AND DATA_PAGAMENTO IS NULL OR DATA_PAGAMENTO = '' order BY DATA_VENCIMENTO;]]>
	</queryString>
